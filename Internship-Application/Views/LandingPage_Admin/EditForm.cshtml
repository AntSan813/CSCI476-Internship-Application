@model List<Internship_Application.Models.FormJsonModel>

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>Forms</h4>
    <hr />
    @*<h4>@Html.DisplayNameFor(model => Model.Template.Templates[0].TemplateName)</h4>*@
    <hr />
    <table class="table">
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Prompt)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.PromptValue)
                    </td>
                </tr>
            }
        </tbody>
        </table>
</div>
<div>
    <a asp-action="Index">Back to List</a>
</div>
